<!--pages/map/map.wxml-->
<!-- <text>pages/map/map.wxml</text> -->
<!-- longituded经度   latitude纬度   scale缩放级别3~20-->
<!-- polyline折线 -->
<map bindtap="local" id="map" 
longituded="{{longituded}}"     
latitude="{{latitude}}"
scale="10"
show-location="true"
style="width:100%;height:600px;"
polyline="{{polyline}}"
circles="{{circles}}"
markers="{{markers}}"
>
</map>